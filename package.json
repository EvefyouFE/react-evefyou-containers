    "@ant-design/icons": ">=5.0.1",
    "@dnd-kit/core": ">=6.0.8",
    "@dnd-kit/modifiers": ">=6.0.1",
    "@dnd-kit/sortable": ">=7.0.2",
    "@dnd-kit/utilities": ">=3.2.1",
    "@emotion/css": ">=11.11.0",
    "ahooks": ">=3.7.6",
    "antd": ">=5.6.3",
    "classnames": ">=2.3.2",
    "ramda": ">=0.29.0",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0",
    "react-draggable": ">=4.4.5",
    "react-intl": ">=6.4.4",
    "react-router": ">=6.10.0",
    "react-router-dom": ">=6.10.0",
    "uuid": ">=9.0.0",
    "react-evefyou-common": ">=1.0.18",
    "react-evefyou-hooks": ">=1.0.29",
    "react-evefyou-components": ">=1.0.47",
    "@iconify/react": ">=4.1.1",
    "moment": ">=2.29.4",
    "nprogress": ">=0.2.0"
  },
